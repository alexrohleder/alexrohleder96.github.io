    body

        div.foreground
            div.foreground-logotype.animated.infinite.flash="ALEXROHLEDER"

        div.content
            header.header.p-t-lg(style="background-image: url(img/background.jpg")
                nav.navbar.navbar-transparent.navbar-theme
                    div.container
                        a.navbar-brand(href) ALEXROHLEDER
                        a.navbar-toggler.hidden-md-up.pull-right ☰
                        div.navbar-toggleable-sm.collapse
                            ul.navbar-nav.nav.pull-right
                                li.nav-item: a.nav-link(href="#about") sobre
                                li.nav-item: a.nav-link(href="#skills") habilidades
                                li.nav-item: a.nav-link(href="#experiense") experiência
                                li.nav-item: a.nav-link(href="#portfolio") portfólio
                                li.nav-item: a.nav-link(href="#blog") blog
